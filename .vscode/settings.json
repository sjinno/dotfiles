{
    "C_Cpp.updateChannel": "Insiders",
    "[python]": {
        "editor.wordBasedSuggestions": false,
        "gitlens.codeLens.symbolScopes": ["!Module"]
    },
    "[rust]": {
        "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "debug.allowBreakpointsEverywhere": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "'Source Code Pro'",
    "editor.fontLigatures": "'ss01",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    // "editor.lineHeight": 18,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "editor.scrollbar.verticalScrollbarSize": 10,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            // "*.constant": {
            //     "fontStyle": "bold underline",
            //     "foreground": "#1d4ed8"
            // },
            "comment": "#52525b",
            "enum": "#92400e",
            "enumMember": "#92400e",
            // "interface": {
            // "fontStyle": "italic"
            // "foreground": "#047857"
            // },
            // "keyword": "#dc2626",
            "macro": {
                "fontStyle": "bold",
                "foreground": "#4338ca"
            },
            "string": "#047857",
            "typeAlias": {
                "fontStyle": "italic bold",
                "foreground": "#92400e"
            }
        }
    },
    "editor.suggestSelection": "first",
    // "editor.tokenColorCustomizations": {
    //     "textMateRules": [
    //         {
    //             "name": "Markdown",
    //             "scope": [
    //                 "meta.paragraph.markdown",
    //                 "string.other.link.title.markdown",
    //                 "markup.underline.link.markdown"
    //             ],
    //             "settings": {
    //                 "fontStyle": "italic"
    //             }
    //         }
    //     ]
    // },
    "editor.wordWrap": "on",
    "files.associations": {
        "*.css": "scss",
        "*.drawio": "javascript",
        "*.js": "javascript"
    },
    "files.encoding": "utf8",
    "git.autofetch": true,
    "githd.blameView.enabled": false,
    "github.copilot.enable": {
        "*": false,
        "markdown": false,
        "plaintext": false,
        "yaml": false
    },
    "gitlens.currentLine.enabled": false,
    "hediet.vscode-drawio.local-storage": {
        ".drawio-config": {
            "autosave": true,
            "createTarget": false,
            "customFonts": [],
            "customLibraries": ["L.scratchpad"],
            "darkGridColor": "#6e6e6e",
            "formatWidth": "240",
            "gridColor": "#d0d0d0",
            "isRulerOn": false,
            "language": "",
            "libraries": "general;uml;er;bpmn;flowchart;basic;arrows2",
            "openCounter": 0,
            "pageFormat": {
                "height": 1100,
                "width": 850,
                "x": 0,
                "y": 0
            },
            "plugins": [],
            "recentColors": [],
            "resizeImages": null,
            "search": true,
            "showStartScreen": true,
            "ui": "",
            "unit": 1,
            "version": 18
        }
    },
    "hediet.vscode-drawio.theme": "dark",
    "helium-icon-theme.saturation": 0.8,
    "html.format.templating": true,
    "http.proxyAuthorization": null,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "latex-workshop.latex.autoBuild.run": "never",
    "launch": {},
    "liveSassCompile.settings.formats": [
        {
            "extensionName": ".min.css",
            "format": "compressed",
            "savePath": "/css"
        }
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "prettier.semi": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
    "rust-analyzer.inlayHints.enable": false,
    "rust-analyzer.inlayHints.maxLength": 6,
    "terminal.integrated.fontFamily": "Source Code Pro",
    "terminal.integrated.fontSize": 14,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.menuBarVisibility": "toggle",
    "window.title": "${rootName}${separator}${activeEditorMedium}",
    "workbench.activityBar.visible": true,
    "workbench.colorCustomizations": {
        "activityBar.background": "#f6f8fa", // ACTIVITY BAR
        "activityBar.foreground": "#1b1f23",
        "editor.background": "#f3f0df", // GITHUB PLUS *****
        "editorGroupHeader.tabsBackground": "#f0f0f0", // TABS BACKGROUND
        "editorWhitespace.foreground": "#a3b18a", // Laurel green
        "editor.lineHighlightBackground": "#e4e4e7",
        "editorLineNumber.foreground": "#a1a1aa",
        "editorLineNumber.activeForeground": "#ff006e",
        "scrollbarSlider.background": "#a3aab1",
        "sideBar.background": "#f0f0f0", // SIDE BAR
        "statusBar.background": "#33312e",
        "statusBar.debuggingBackground": "#33312e",
        "statusBar.foreground": "#ffffff",
        "tab.activeBackground": "#fafbfc",
        "tab.inactiveBackground": "#f0f0f0"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "helium-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontSize": 12.5,
    "workbench.colorTheme": "GitHub Plus",
    "debug.disassemblyView.showSourceCode": false,
}
